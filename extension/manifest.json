{
    "name": "Generate PR Description Easy",
    "version": "0.0.0.4",
    "manifest_version": 3,
    "description": "An extension to facilite the life.",
    "permissions": [
        "sidePanel"
    ],
    "icons": {
        "16": "icon16x16.png",
        "32": "icon32x32.png",
        "48": "icon48x48.png"
    },
    "side_panel": {
        "default_path": "teste-pr.html"
    },
    "action": {
        "default_popup": "teste-pr.html"
    }
}