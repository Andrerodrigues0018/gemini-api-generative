{
    "name": "Gerador de título e descrição para Pull Requests",
    "version": "1.0.0.0",
    "manifest_version": 3,
    "description": "Automatize Títulos e Descrições para Seus Pull Requests!",
    "permissions": [
        "sidePanel"
    ],
    "icons": {
        "16": "icon16x16.png",
        "32": "icon32x32.png",
        "48": "icon48x48.png"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "action": {
        "default_popup": "index.html"
    }
}